
{
    "installCommand": "pip install -r requirements.txt",
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "rewrites": [
      {
        "source": "(.*)",
        "destination": "main.py"
      }
    ],
    "trailingSlash": true,
    "cleanUrls": true
  }