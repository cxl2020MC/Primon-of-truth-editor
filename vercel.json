{
  "installCommand": "pip install -r requirements.txt",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main"
    },
    {
      "source": "/docs",
      "destination": "/api/main"
    },
    {
      "source": "/redoc",
      "destination": "/api/main"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/main"
    }
  ],
  "cleanUrls": true
}